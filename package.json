{
    "name": "cappuccino",
    "displayName": "Cappuccino",
    "description": "VS Code extension for Cappuccino, includes Objective-J language support",
    "icon": "icon.png",
    "version": "0.0.9",
    "publisher": "rramsden",
    "engines": {
        "vscode": "^1.18.0"
    },
    "keywords": [
        "Objective-J",
        "objj",
        "Cappuccino"
    ],
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "vscode:prepublish": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rramsden/vscode-cappuccino.git"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:objj"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "objj",
                "aliases": [
                    "Objective-J",
                    "objj"
                ],
                "extensions": [
                    ".j",
                    ".J"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "snippets": [
            {
                "language": "objj",
                "path": "./snippets/objj.json"
            }
        ],
        "grammars": [
            {
                "language": "objj",
                "scopeName": "source.js.objj",
                "path": "./syntaxes/Objective-J.tmLanguage"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^8.0.57",
        "typescript": "^2.6.2",
        "vscode": "^1.1.10"
    },
    "dependencies": {
        "acorn-objj": "^1.1.3"
    }
}
